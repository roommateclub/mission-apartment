<div class="row">
  <div class="col-xs-12">
    <table class="table">
      <thead>
        <tr>
          <th>id</th>
          <th>姓名</th>
          <th>email</th>
        </tr>
      </thead>
      <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
    
  </div>
</div>